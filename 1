<VirtualHost *:80>
ServerAdmin webmaster@example.com
DocumentRoot /home/ubuntu/django/django_sale
ErrorLog ${APACHE_LOG_DIR}/error.log
CustomLog ${APACHE_LOG_DIR}/access.log combined
Alias /static /home/ubuntu/django/django_sale/static
<Directory /home/ubuntu/django/django_sale/static>
Require all granted
</Directory>
<Directory /home/ubuntu/django/django_sale/potrip>
<Files wsgi.py>
Require all granted
</Files>
</Directory>
WSGIDaemonProcess django_sale python-path=/home/ubuntu/django/django_sale python-home=/home/ubuntu/django/django
WSGIProcessGroup django_sale
WSGIScriptAlias / /home/ubuntu/django/myproject/myproject/wsgi.py
</VirtualHost>